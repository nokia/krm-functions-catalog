    "rw": "^1.3.3"
  },
  "devDependencies": {
    "@types/jasmine": "^3.8.1",
    "@types/js-yaml": "^3.12.7",
    "@types/node": "^14.17.12",
    "@types/rw": "1.3.3",
    "create-kpt-functions": "^0.18.0",
    "jasmine": "^3.9.0",
    "license-checker": "^25.0.1",
    "prettier": "2.3.2",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-consistent-codestyle": "^1.16.0",
    "typescript": "~4.4.2"
  },
